{% extends 'base.html.twig' %}


{% block container %}
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}ecosmart Maps {% endblock %}</title>
    {% block stylesheets %}
        {{ parent() }}

        <link rel="stylesheet" href="{{ asset('map/css/style.css') }}" />
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    {% endblock %}

    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDaNEKwnE6RixLvi7quETn6c2SJX3BGzmI&libraries=places"></script>
    {% block SubHeader %}
        <!--// SubHeader \\-->
        <div class="environment-subheader">
            <span class="subheader-transparent"></span>
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <h1>EcoSmaart+</h1>
                    </div>
                    <div class="col-md-12">
                        <ul class="environment-breadcrumb">
                            <li><a href="index-2.html">Map</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <!--// SubHeader \\-->
    {% endblock %}
</head>
<body class="centered-wrapper">
{% block body %}{% endblock %}
{% block javascripts %}
 {{ parent() }}
    <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script src="{{ path('fos_js_routing_js', { callback: 'fos.Router.setData' }) }}"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="{{ asset('map/js/map.js') }}" type="text/javascript"></script>
{% endblock %}
</body>
</html>
{% endblock %}